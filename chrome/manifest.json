{
	"name": "Photo Booth - The New Yorker",
	"short_name": "Photo Booth",
	"description": "The best of The New Yorker's photo blog, each time you open a tab.",
	"version": "0.0.3",
	"icons": {
		"256": "content/img/icon256.png",
		"128": "content/img/icon128.png",
		 "64": "content/img/icon64.png"
	},
	"manifest_version": 2,
	"author": "Michael Donohoe",
	"chrome_url_overrides": {
		"newtab": "content/index.html"
	},
	"content_security_policy": "script-src 'self' https://dl.dropboxusercontent.com; object-src 'self'",
	"permissions": [ "identity", "https://dl.dropboxusercontent.com/*" ]
}
